{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "google"
  ],
  "rules": {
    "semi": "off",
    "linebreak-style": "off",
    "object-curly-spacing": "off",
    "comma-dangle": "off",
    "arrow-parens": "off",
    "require-jsdoc": "off",
    "space-before-function-paren": [
      "error",
      {
        "anonymous": "always",
        "named": "never",
        "asyncArrow": "always"
      }
    ],
    "indent": [
      2,
      2,
      {
        "SwitchCase": 2
      }
    ],
    "operator-linebreak": [
      "error",
      "before"
    ]
  }
}
